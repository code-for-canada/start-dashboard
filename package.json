{
  "name": "start-dashboard",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "setup": "cp -n sample.env .env || echo .env file already exists. Skipping...",
    "start": "node server",
    "build": "cd react-ui/ && npm install && npm run build",
    "start:ui": "cd react-ui && npm run start",
    "start:server": "nodemon server",
    "start:dev": "concurrently --kill-others-on-fail 'npm run start:ui' 'PORT=8080 npm run start:server'",
    "start:prod": "npm run build && NODE_ENV=production npm start"
  },
  "dependencies": {
    "airtable": "^0.10.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^1.11.0",
    "jwt-decode": "^3.0.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "prettier-config-standard": "^1.0.1"
  },
  "cacheDirectories": [
    "node_modules",
    "react-ui/node_modules"
  ],
  "license": "MIT"
}
